body
{
	font-family: 'Roboto', sans-serif;
	font-weight:400;
}
*{outline:none;}
.visible-mobile{display:none;}
a,a:hover, a:focus{text-decoration:none;}
a:hover{color:#bd0801;}
.green{color:#34a52f;}
.btn-yellow{font-size:16px; line-height:40px; background:#febc1d; padding:0px 20px;}
.btn-yellow:hover{background:none; border:1px solid #febc1d; color:#febc1d}
header{background:#ffbd1e;}
header .inner-header:after{content:" "; display:block; clear:both;}
header .inner-header .sign-up{margin:25px 0px 0px;}
header .inner-header .sign-up ul{margin:0px; padding:0px;}
header .inner-header .sign-up ul:after{content:" "; display:block; clear:both;}
header .inner-header .sign-up ul li{list-style-type:none; display:block; float:left; margin:0px 40px 0px 0px; font-size:18px;}
header .inner-header .sign-up ul li a{display:block; color:#34302f; font-size:18px;}
header .inner-header .sign-up ul li a:hover{color:#bd0801;}
header .inner-header .sign-up ul li:last-child{margin:-13px 0px 0px;}
header .inner-header .sign-up ul .join{margin:0px;}
header .inner-header .sign-up ul .or{margin:0px 5px;}
header .inner-header .sign-up ul li .bg{background:url(../images/cart-shopping.gif) no-repeat; width:64px; height:43px; text-align:right;}
header .inner-header .sign-up ul li .bg span{display:block; color:#ffffff; padding:4px 12px 0px 0;}
.delivery{background:rgba(0,0,0,0.7);}
.delivery .inner-delivery{padding:9px 0px;}
.delivery .inner-delivery:after{content:" "; display:block; clear:both;}
.delivery .inner-delivery p{margin:0px; font-size:14px; color:#ffffff;  margin:7px 23px 0px 0px;}
.delivery .inner-delivery .change-address{line-height:31px; font-size:16px; background:transparent; border:1px solid #676566; display:block; padding:0px 11px; color:#ffffff;}
.delivery .inner-delivery .change-address:hover{color:#bd0801;}
.check-it .container{position:relative;}
.check-it .rewards{line-height:40px; font-weight:500; font-size:22px; color:#ffffff; background:#bd0800; position:absolute; margin-top:52px; right:0px; padding:0px 9px; box-shadow:1px 1px 4px 0px rgba(0,0,0,0.7), -1px -1px 4px 0px rgba(0,0,0,0.7); border:none; z-index:999;}
.check-it .rewards:hover{color:#ffbd1e;}
.check-it .rewards:before{content:url(../images/rewards.png) no-repeat center top; display:block;position: absolute; top: -52px; right: 0; left: 0;}
.check-it.rewards:after{content:" "; display:block; clear:both;}
.check-it .rewards .inner-links ul{margin:0px; padding:0px; font-size:0px;} 
.check-it.rewards .inner-links ul li{list-style-type:none; display:inline-block;}
.categories .banner{background:url(../images/banner.png) no-repeat center center; background-size:cover; height:200px;}
.banner .delivery{margin:0px 0px 6px;}
.page-indication .breadcrumb {
  background: none;
  border-radius: 0px;
  padding: 0px;
  margin: 0px;
  display: block;
}
.page-indication .breadcrumb:after {
  content: " ";
  display: block;
  clear: both;
}
.page-indication .breadcrumb >li+li:before {
  display: block;
  float: left;
  line-height: 18px;
  content:">";
}
.page-indication .breadcrumb li {
  display: block;
  float: left;
}
.page-indication .breadcrumb li a {
  line-height: 18px;
  font-size: 16px;
  color: #ffffff;
  display: block;
  float: left;
  text-shadow:1px 1px 1px rgba(0,0,0,0.4);
}
.page-indication .breadcrumb li:hover a {
  color:#bd0801;
}
.page-indication .breadcrumb .active a {
  color:#bd0801;
}

.banner .carousel .carousel-inner .item {
  height: 500px;
  background-size: cover;
  background-position: center top;
  background-repeat: no-repeat;
}
.banner .carousel .carousel-inner .item .carousel-caption {
  right: 0;
  left: 0;
  padding: 0;
  top: 0px;
  bottom: 0px;
  text-shadow: none;
  text-align: inherit;
}
.banner .carousel .carousel-inner .item .carousel-caption .container {
  height: 100%;
}
.banner .carousel .carousel-inner .item .carousel-caption .center-align {
  display: table;
  width: 100%;
  height: 100%;
}
.banner .carousel .carousel-inner .item .carousel-caption .center-align .caption-text {
  display: table-cell;
  vertical-align: middle;
  text-align:center;
}
.banner .carousel .carousel-inner .item .carousel-caption .center-align .caption-text h1 {
	 font-family: 'Lobster', cursive;
	 font-weight:400;
	 line-height:40px;
	 margin:0px 0px 19px;
	 font-size:40px;
	 color:#ffffff;
	 text-shadow:2px 2px 4px rgba(0,0,0,0.8); 
}

.banner .carousel .carousel-inner .item .carousel-caption .center-align .caption-text p
{
	line-height:31px;
	margin:0px;	
	text-shadow:2px 2px 4px rgba(0,0,0,0.8);
	font-size:26px;
	color:#ffffff;
	margin:0px 0px 50px;
}
.banner .item .location .form-inline{font-size:0px; text-align:initial; padding:10px 20px 10px 89px; position:relative;}
.banner .item .location .form-inline:before{content:" "; display:block; position:absolute;left:14px; top:-44px; background-image:url(../images/location.png); background-repeat:no-repeat; width:63px; height:94px;}
.banner .item .location{width:850px; background:rgba(0,0,0,0.6); margin:0 auto;}
.banner .item .location .form-inline ::-webkit-input-placeholder {
   color:#333333;
}
.banner .item .location .form-inline :-moz-placeholder { /* Firefox 18- */
   color:#333333;  
}
.banner .item .location .form-inline ::-moz-placeholder {  /* Firefox 19+ */
   color:#333333;  
}
.banner .item .location .form-inline :-ms-input-placeholder {  
   color:#333333;  
}
.banner .item .location .form-inline .form-group{padding:0px 10px 0px 0px; width:50%;}
.banner .item .location .form-inline .form-group:last-child{padding:0px 0px 0px 10px;}
.banner .item .location .form-inline .form-group .help-block{margin:0px;}
.banner .item .location .form-inline .form-group .form-control{height:52px; line-height:52px; padding:0px 12px; font-size:24px; color:#333333; border:0px; width:100%; -webkit-appearance: none;}

