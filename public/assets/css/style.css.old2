.modal-body .thank-you p{line-height:21px; margin:0px 0px 20px; font-size:15px;}
.sign-up .modal-body{margin:0px 0px 40px;}
.sign-up .modal-body .form-horizontal .form-group{float:left; margin:0px 18px 0px 0px;}
