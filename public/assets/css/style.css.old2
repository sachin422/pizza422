.modal-header ul li:hover{border-color:#ffb922;}
.modal-header ul li:hover a{color:#ffb922;}
.modal-header ul li a{display:block; color:#989898;}
.modal-header button{position:absolute; right:-10px; top:-12px; margin-top:0px !important; opacity:1.0;}
.modal-header button:hover{opacity:1.0;}
.modal-body{padding:20px;}
.modal-body .block-form{width:100%;}
