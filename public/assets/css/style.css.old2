.modal-body .checkbox .forgot:hover{color:#bd0801;}
.modal-body .block-form .btn-yellow{margin:0px 0px 0px 28px;}
.modal-body .form-group.remember-me{margin:0px 0px 72px;}
.modal-footer{text-align:center; border:0px;}
.modal-footer .sign-up{color:#ffa81a; font-size:16px; text-decoration:underline; padding-left:8px;}
.modal-footer .sign-up:hover{color:#bd0801;}
.sign-up .modal-body{padding:20px 20px 0px;}
.modal-body .thank-you{text-align:center;}
