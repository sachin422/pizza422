.modal-dialog{margin:50px auto 0px; width:400px;}
.modal-content{border-bottom:3px solid #f9c232; background:#ffffff url(../images/login-bg.png) no-repeat center bottom; border-radius:0px; padding:0px 0px 132px;}
.modal-header{border-bottom:1px solid #ffbb20; position:relative; padding:0px;}
.modal-header ul{margin:0px; padding:0px;}
.modal-header ul:after{content:" "; display:block; clear:both;}
.modal-header ul li{list-style-type:none; display:block; float:left; width:50%; text-align:center; border-bottom:5px solid transparent; padding:24px 0px 8px; font-size:16px;}
.modal-header ul li.active{border-color:#ffb922;}
